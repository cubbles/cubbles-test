{
    "name": "connectionhook-scenario",
    "groupId": "cubx.rte.test",
    "version": "0.1.0-SNAPSHOT",
    "modelVersion": "8.3.0",
    "docType": "webpackage",
    "author": {
        "name": "Judit Ross",
        "email": "judit.ross@incowia.com"
    },
    "license": "MIT",
    "keywords": [],
    "man": [],
    "artifacts": {
        "apps": [],
        "elementaryComponents": [
            {
                "artifactId": "message-viewer",
                "description": "This is an elementary-component based on Polymer.",
                "runnables": [
                    {
                        "name": "demo",
                        "path": "/demo/index.html",
                        "description": "Demo app..."
                    }
                ],
                "endpoints": [
                    {
                        "endpointId": "main",
                        "description": "This is recommended for you use with webcomponents.",
                        "resources": [
                            "message-viewer.html"
                        ],
                        "dependencies": [
                            "cubx.core.rte@1.5.0-SNAPSHOT/cubxpolymer/main",
                            "bootstrap-3.3.5@1.0.3/bootstrap/bootstrap-css-only"
                        ]
                    }
                ],
                "slots": [
                    {
                        "slotId": "message",
                        "type": "string",
                        "direction": [
                            "input",
                            "output"
                        ]
                    }
                ]
            }
        ],
        "compoundComponents": [
            {
                "artifactId": "message-viewer-compound",
                "description": "This is a cubbles-compound-component.",
                "runnables": [
                    {
                        "name": "demo",
                        "path": "/demo/index.html",
                        "description": "Demo app..."
                    }
                ],
                "endpoints": [
                    {
                        "endpointId": "main",
                        "resources": [
                            "css/message-viewer-compound.css"
                        ],
                        "dependencies": [
                            "this/message-viewer/main"
                        ]
                    }
                ],
                "slots": [
                    {
                        "slotId": "message",
                        "type": "string",
                        "direction": [
                            "input",
                            "output"
                        ]
                    }
                ],
                "members": [
                    {
                        "memberId": "inner",
                        "componentId": "this/message-viewer"
                    }
                ],
                "connections": [
                    {
                        "connectionId": "internal-ingoing",
                        "source": {
                            "slot": "message"
                        },
                        "destination": {
                            "memberIdRef": "inner",
                            "slot": "message"
                        },
                        "hookFunction": "function(value,next){ var newValue = value + ' (4)'; next(newValue);}"
                    },
                    {
                        "connectionId": "internal-outgoing",
                        "source": {
                            "memberIdRef": "inner",
                            "slot": "message"
                        },
                        "destination": {
                            "slot": "message"
                        },
                        "hookFunction": "function(value,next){ var newValue = value + ' (5)'; next(newValue);}"
                    }
                ],
                "inits": []
            },
            {
                "artifactId": "example-compound",
                "description": "This is a cubbles-compound-component.",
                "runnables": [
                    {
                        "name": "example",
                        "path": "/demo/index.html",
                        "description": "Example for connection hookFunctions, across sibling connections and internal connections"
                    }
                ],
                "endpoints": [
                    {
                        "endpointId": "main",
                        "resources": [
                            "css/example-compound.css",
                            "js/style.js"
                        ],
                        "dependencies": [
                            "this/message-viewer/main",
                            "this/message-viewer-compound/main"
                        ]
                    }
                ],
                "members": [
                    {
                        "memberId": "first",
                        "componentId": "this/message-viewer"
                    },
                    {
                        "memberId": "second",
                        "componentId": "this/message-viewer-compound"
                    },
                    {
                        "memberId": "third",
                        "componentId": "this/message-viewer-compound"
                    },
                    {
                        "memberId": "fourth",
                        "componentId": "this/message-viewer"
                    }
                ],
                "connections": [
                    {
                        "connectionId": "firstToSecond",
                        "source": {
                            "memberIdRef": "first",
                            "slot": "message"
                        },
                        "destination": {
                            "memberIdRef": "second",
                            "slot": "message"
                        },
                        "hookFunction": "function(value,next){ var newValue = value + ' (1)'; next(newValue);}"
                    },
                    {
                        "connectionId": "secondToThird",
                        "source": {
                            "memberIdRef": "second",
                            "slot": "message"
                        },
                        "destination": {
                            "memberIdRef": "third",
                            "slot": "message"
                        },
                        "hookFunction": "function(value,next){ var newValue = value + ' (2)'; next(newValue);}"
                    },
                    {
                        "connectionId": "thirdToForth",
                        "source": {
                            "memberIdRef": "third",
                            "slot": "message"
                        },
                        "destination": {
                            "memberIdRef": "fourth",
                            "slot": "message"
                        },
                        "hookFunction": "function(value,next){ var newValue = value + ' (3)'; next(newValue);}"
                    }
                ],
                "inits": []
            }
        ],
        "utilities": []
    }
}
