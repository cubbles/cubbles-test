              "memberIdRef": "member-a",
              "slot": "c"
            },
            "destination": {
              "memberIdRef": "member-b",
              "slot": "c"
            },
            "hookFunction": "function(value, next) { var newValue = {}; newValue.name = value.firstname ? value.firstname + ' ' : ''; newValue.name += value.name; next(newValue);};"
          }
